   "source": [
    "class Net(nn.Module):\n",
    "    def __init__(self, n=100):\n",
    "        super(Net, self).__init__()\n",
    "        self.fc1 = nn.Linear(784, n)\n",
    "        self.fc2 = nn.Linear(n, 10)\n",
    "        \n",
    "    def forward(self, X):\n",
    "\n",
    "        X1 = F.relu(self.fc1(X))\n",
    "        return self.fc2(X1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
   ],
   "source": [
    "model2 = Net().to(device)"
   ]
  },
